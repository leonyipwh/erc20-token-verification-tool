{
  "settings": {
    "react": {
    "pragma": "React",
    "version": "16.12.0"
    }
  },
  "parser":  "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "react"],
  "extends": ["eslint:recommended","plugin:react/recommended","plugin:react/jsx-runtime","plugin:jest/recommended", "prettier"],
  "env": {
    "node": true,
    "jest/globals": true,
    "browser": true
  },
  "rules": {
   "react/react-in-jsx-scope": "off",
   "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }],
   "no-unused-vars": "off",
   "@typescript-eslint/no-unused-vars":["error"]
  }
}